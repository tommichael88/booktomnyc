        };
        const targetedSuggestions = {
          "Appliances ‚è∞ Hourly": ["Refrigerator Repair", "Washing Machine Repair", "Dryer Repair", "Dishwasher Repair", "Oven Repair", "Stove Repair", "Microwave Repair", "Garbage Disposal Repair", "Freezer Repair", "Ice Maker Repair", "Range Hood Repair", "Wine Cooler Repair", "Trash Compactor Repair"],
